<li class='clearfix record <%= cycle("on", "on-hover") %>' id="<%= dom_id(guide) -%>">
  <span class='title'>
    <%= guide.title %>
    <span class="preview">&nbsp;</span>
  </span>
  <span class='actions'>
    <%= link_to refinery_icon_tag("application_go.png"), guide_url(guide),
        :title => t('.view_live_html'),
        :target => "_blank" %>
    <%= link_to refinery_icon_tag("application_edit.png"), edit_admin_guide_path(guide),
         :title => t('.edit') %>
    <%= link_to refinery_icon_tag("delete.png"), admin_guide_path(guide),
        :class => "cancel confirm-delete",
        :title => t('.delete'),
        :'data-confirm' => t('shared.admin.delete.message', :title => guide.title),
        :'data-method' => :delete %>
  </span>
</li>
